@model IEnumerable<DataLayer.Workflow>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WorkflowName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkflowDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Disabled)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WorkflowName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WorkflowDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Disabled)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.WorkflowID }) |
            @Html.ActionLink("Details", "Details", new { id=item.WorkflowID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.WorkflowID })
        </td>
    </tr>
}

</table>
